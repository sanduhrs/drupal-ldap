<?php

/**
 * @file
 * Contains ldap_views.module..
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function ldap_views_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the ldap_views module.
    case 'help.page.ldap_views':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('The LDAP Views.') . '</p>';
      return $output;

    default:
  }
}
